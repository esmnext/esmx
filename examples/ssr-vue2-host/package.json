{
    "name": "ssr-vue2-host",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "license": "MIT",
    "scripts": {
        "dev": "esmx dev",
        "build": "npm run build:dts && npm run build:ssr",
        "build:ssr": "esmx build",

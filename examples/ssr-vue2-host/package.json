        "@esmx/rspack-vue": "workspace:*",
        "@types/express": "^4.17.21",
        "@types/node": "^20.6.3",
        "less": "^4.2.0",
        "ssr-vue2-remote": "workspace:*",
        "typescript": "^5.2.2",
        "vue": "2.7.16",
        "vue-server-renderer": "2.7.16",
        "vue-tsc": "^2.1.6"
    }
}
